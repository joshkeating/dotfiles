! Display settings

Xft.dpi: 96
Xft.autohint: false
Xft.lcdfilter:  lcddefault
Xft.hintstyle:  hintslight
Xft.hinting: true
Xft.antialias: true
Xft.rgba: rgb

! Define terminal/vim  colors

! special
*.foreground:   #e6e1dc
*.background:   #262121
*.cursorColor:  #e6e1dc

! black
*.color0:       #5a647e
*.color8:       #5a647e

! red
*.color1:       #da4939
*.color9:       #da4939

! green
*.color2:       #a5c261
*.color10:      #a5c261

! yellow
*.color3:       #ffc66d
*.color11:      #ffc66d

! blue
*.color4:       #6d9cbe
*.color12:      #6d9cbe

! magenta
*.color5:       #b6b3eb
*.color13:      #b6b3eb

! cyan
*.color6:       #519f50
*.color14:      #519f50

! white
*.color7:       #e6e1dc
*.color15:      #f9f7f3

! terminal emulator settings
URxvt.font: xft:Iosevka:regular:size=16
URxvt.letterSpace: 0
URxvt.scrollStyle:          rxvt
URxvt.scrollBar:            false
URxvt.cursorBlink:          true

! ------------------------------------------------------------------------------
! ROFI Color theme
! ------------------------------------------------------------------------------
rofi.color-enabled: true
rofi.color-window: #262121, #6d9cbe, #262121
rofi.color-normal: #262121, #6d9cbe, #262121, #6d9cbe, #262121
rofi.color-active: #262121, #6d9cbe, #262121, #6d9cbe, #262121
rofi.color-urgent: #262121, #6d9cbe, #262121, #6d9cbe, #262121
